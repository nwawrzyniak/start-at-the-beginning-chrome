{
  "manifest_version": 3,
  "name": "Start at the beginning",
  "description": "A Chrome extension that lets you start YouTube videos at the beginning.",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "graphics/nav_bar.png"
  },
  "icons": {
    "16": "graphics/satb_16.png",
    "32": "graphics/satb_32.png",
    "48": "graphics/satb_48.png",
    "128": "graphics/satb_128.png"
  },
  "content_scripts": [
    {
      "js": [
        "content-scripts/satb.js"
      ],
      "matches": [
        "https://*.youtube.com/watch*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
